{
  "name": "erp-detachement-europe-backend-skeleton",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "postinstall": "node scripts/prune-node-modules-md.cjs",
    "lint": "eslint --max-warnings=0 --no-error-on-unmatched-pattern \"src/**/*.ts\" \"tests/**/*.ts\" \"modules/**/*.ts\" \"scripts/**/*.ts\"",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "build": "tsc -p tsconfig.json",
    "test:ci": "npm run build && node dist/scripts/run-tests.js",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/src/index.js"
  },
  "devDependencies": {
    "@types/node": "^20.17.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^8.57.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
